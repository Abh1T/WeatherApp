        app:layout_constraintBottom_toBottomOf="@+id/high2"
        app:layout_constraintEnd_toEndOf="@+id/pic2"
        app:layout_constraintStart_toStartOf="@+id/pic2"
        android:background="#000000"
        android:textColor="#00b3ff"
        app:layout_constraintTop_toTopOf="@+id/high2" />

    <TextView
        android:id="@+id/date5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/pic5"
        app:layout_constraintEnd_toEndOf="@+id/pic5"
        app:layout_constraintHorizontal_bias="0.518"
        app:layout_constraintStart_toStartOf="@+id/pic5"
        app:layout_constraintTop_toBottomOf="@+id/high1"
        android:background="#000000"
        android:textColor="#00b3ff"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/high2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/pic3"
        app:layout_constraintHorizontal_bias="0.518"
        app:layout_constraintStart_toStartOf="@+id/pic3"
        app:layout_constraintTop_toBottomOf="@+id/pic3"
        android:background="#000000"
        android:textColor="#00b3ff"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/low2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/date5"
        app:layout_constraintEnd_toEndOf="@+id/high2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/high2"
        app:layout_constraintTop_toBottomOf="@+id/high2"
        android:background="#000000"
        android:textColor="#00b3ff"
        app:layout_constraintVertical_bias="0.066" />

    <TextView
        android:id="@+id/high3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
