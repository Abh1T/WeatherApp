        app:layout_constraintTop_toTopOf="@+id/low4"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/low4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#000000"
        android:text="TextView"
        android:textColor="#00b3ff"
        app:layout_constraintBottom_toBottomOf="@+id/high5"
        app:layout_constraintEnd_toStartOf="@+id/low2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/date5"
        app:layout_constraintTop_toTopOf="@+id/high5"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/high5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#000000"
        android:text="TextView"
        android:textColor="#00b3ff"
        app:layout_constraintBottom_toBottomOf="@+id/low5"
        app:layout_constraintEnd_toStartOf="@+id/low5"
        app:layout_constraintHorizontal_bias="0.705"
        app:layout_constraintStart_toStartOf="@+id/low2"
        app:layout_constraintTop_toTopOf="@+id/low5"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/low5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#000000"
        android:text="TextView"
        android:textColor="#00b3ff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.753"
        app:layout_constraintStart_toStartOf="@+id/date6"
        app:layout_constraintTop_toBottomOf="@+id/pic6"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/forecast"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#00b3ff"
        app:layout_constraintBottom_toTopOf="@+id/date1"
        app:layout_constraintEnd_toEndOf="@+id/date1"
        app:layout_constraintStart_toStartOf="@+id/date1"
        android:background="#000000"
        app:layout_constraintTop_toBottomOf="@+id/pic1" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="145dp"
        android:layout_height="19dp"
        android:layout_marginTop="16dp"
        android:background="#000000"
        android:ems="10"
        android:hint="Zip Code"
        android:inputType="textPersonName"
        android:text="Zip Code"
        android:textColor="#00b3ff"
        app:layout_constraintBottom_toTopOf="@+id/temp1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

