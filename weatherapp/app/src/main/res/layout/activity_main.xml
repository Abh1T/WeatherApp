        android:textColor="#00b3ff"
        app:layout_constraintTop_toTopOf="@+id/high2" />

    <TextView
        android:id="@+id/low3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/low2"
        app:layout_constraintEnd_toEndOf="@+id/high3"
        app:layout_constraintStart_toStartOf="@+id/high3"
        android:background="#000000"
        android:textColor="#00b3ff"
        app:layout_constraintTop_toTopOf="@+id/low2" />

    <TextView
        android:id="@+id/high4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#000000"
        android:text="TextView"
        android:textColor="#00b3ff"
        app:layout_constraintBottom_toBottomOf="@+id/low4"
        app:layout_constraintEnd_toStartOf="@+id/low4"
        app:layout_constraintHorizontal_bias="0.423"
