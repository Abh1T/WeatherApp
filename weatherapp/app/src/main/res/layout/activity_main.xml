    <ImageView
        android:id="@+id/pic5"
        android:layout_width="86dp"
        android:layout_height="82dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/pic3"
        app:layout_constraintHorizontal_bias="0.627"
        app:layout_constraintStart_toStartOf="@+id/pic2"
        app:layout_constraintTop_toBottomOf="@+id/pic2"
        app:layout_constraintVertical_bias="0.794"
        app:srcCompat="@android:drawable/ic_menu_upload" />

    <ImageView
        android:id="@+id/pic6"
        android:layout_width="86dp"
        android:layout_height="82dp"
        app:layout_constraintBottom_toBottomOf="@+id/pic5"
        app:layout_constraintEnd_toEndOf="@+id/pic4"
        app:layout_constraintStart_toEndOf="@+id/pic3"
        app:layout_constraintTop_toTopOf="@+id/pic5"
        app:layout_constraintVertical_bias="0.612"
        app:srcCompat="@android:drawable/ic_menu_upload" />

    <ImageView
        android:id="@+id/pic2"
        android:layout_width="86dp"
        android:layout_height="82dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/pic3"
        app:layout_constraintEnd_toStartOf="@+id/pic3"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/pic3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/ic_menu_upload" />

    <TextView
        android:id="@+id/low1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/low2"
        app:layout_constraintEnd_toEndOf="@+id/high1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/high1"
        app:layout_constraintTop_toBottomOf="@+id/high2"
        android:background="#000000"
        android:textColor="#00b3ff"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/date2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/date3"
        app:layout_constraintEnd_toEndOf="@+id/pic2"
        app:layout_constraintStart_toStartOf="@+id/pic2"
        android:background="#000000"
        android:textColor="#00b3ff"
        app:layout_constraintTop_toTopOf="@+id/date3" />

    <TextView
        android:id="@+id/date3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/pic3"
        app:layout_constraintEnd_toEndOf="@+id/pic3"
        android:textColor="#00b3ff"
        android:background="#000000"
        app:layout_constraintStart_toStartOf="@+id/pic3"
        app:layout_constraintTop_toBottomOf="@+id/date1" />

    <TextView
        android:id="@+id/date4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/date3"
        app:layout_constraintEnd_toEndOf="@+id/pic4"
        app:layout_constraintStart_toStartOf="@+id/pic4"
        android:background="#000000"
        android:textColor="#00b3ff"
        app:layout_constraintTop_toTopOf="@+id/date3" />

    <TextView
        android:id="@+id/date6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/date5"
        app:layout_constraintEnd_toEndOf="@+id/pic6"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="@+id/pic6"
        app:layout_constraintTop_toTopOf="@+id/date5"
        android:background="#000000"
        android:textColor="#00b3ff"
